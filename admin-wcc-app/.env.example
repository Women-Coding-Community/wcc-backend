# Backend API base URL (local dev)
NEXT_PUBLIC_API_BASE=http://localhost:8080
# Example remote API (uncomment to use)
# NEXT_PUBLIC_API_BASE=https://dev.env.url
# Optional: API key if your backend requires it (sent as X-API-KEY header and api_key query)
NEXT_PUBLIC_API_KEY=
# For the public demo backend, you can use:
# NEXT_PUBLIC_API_KEY=api-key-from-dev-env
# App URL (for CORS and cookies when deployed)
NEXT_PUBLIC_APP_URL=
